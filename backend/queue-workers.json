[
    {
        "name": "cron-scheduler",
        "interpreter": "php",
        "script": "artisan",
        "args": [
            "schedule:run"
        ],
        "instances": 1,
        "exec_mode": "fork",
        "cron": "* * * * *",
        "autorestart": false,
        "watch": false,
        "max_memory_restart": "1G"
    },
    {
        "name": "worker-default",
        "interpreter": "php",
        "script": "artisan",
        "args": [
            "queue:work",
            "--tries=3",
            "--timeout=0"
        ],
        "instances": 1,
        "exec_mode": "fork",
        "autostart": true,
        "watch": false,
        "max_memory_restart": "1G"
    },
    {
        "name": "worker-files",
        "interpreter": "php",
        "script": "artisan",
        "args": [
            "queue:work",
            "--name=files",
            "--queue=files",
            "--tries=3",
            "--timeout=0"
        ],
        "instances": 1,
        "exec_mode": "fork",
        "autostart": true,
        "watch": false,
        "max_memory_restart": "1G"
    }
]
